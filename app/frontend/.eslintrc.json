{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["trybe-frontend"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "never",
        "objects": "always",
        "imports": "never",
        "exports": "never",
        "functions": "always"
      }
    ]
  }
}
